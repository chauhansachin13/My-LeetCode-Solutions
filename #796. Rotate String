class Solution {
    public boolean rotateString(String s, String goal) {

        String str="";

        if(s.equals(goal))
        return true;

        for(int i=1;i<s.length();i++)
        {
char ch=s.charAt(i-1);

str=s.substring(i)+s.substring(0,i);

if(str.equals(goal))
return true;


        }
        return false;
       
    }
}
